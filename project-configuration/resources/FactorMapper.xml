<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="org.huangfugui.ibatis.mapper.FactorMapper">
    <select id="selectById" resultType="factor">
        SELECT
          *
        FROM
          factor
        WHERE
          factor_id=#{factorId}
    </select>
</mapper>