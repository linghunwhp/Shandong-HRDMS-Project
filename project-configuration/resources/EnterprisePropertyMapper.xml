<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="org.huangfugui.ibatis.mapper.EnterprisePropertyMapper">
    <select id="selectByPropertyId" resultType="enterpriseProperty">
      SELECT
        *
      FROM
        enterprise_property
      WHERE
        property_id=#{propertyId}
    </select>
</mapper>