<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="org.huangfugui.ibatis.mapper.DecreaseTypeMapper">
    <select id="selectById" resultType="decreaseType">
      SELECT
        de_type_id AS typeId,
        de_type_name AS typeName
      FROM
        decrease_type
      WHERE
        de_type_id=#{typeId}
    </select>
</mapper>